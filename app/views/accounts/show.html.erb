<h1><%= @account.name %></h1>



<% if @ccount.is_a?(Employee) %>
  <h3>Time Entries</h3>
  <table>
    <% @account.time_entries.each do |entry| %>
    <tr>
      <td><%= entry.inspect %></td>
    </tr>
    <% end %>
  </table>
<% else %>
  <h3>Account Entries</h3>
  <table>
    <% @account.account_entries.each do |entry| %>
    <tr>
      <td><%= entry.inspect %></td>
    </tr>
    <% end %>
  </table>
<% end %>

